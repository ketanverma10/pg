<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".register">
    <ImageView
        android:id="@+id/logo"
        android:layout_width="105dp"
        android:layout_height="114dp"
        android:layout_marginTop="76dp"
        android:background="@android:color/transparent"
        android:padding="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/loginas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:autofillHints=""
        android:drawablePadding="15dp"
        android:ems="10"
        android:hint="REGISTER AS "
        android:minHeight="48dp"
        android:paddingStart="90dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo"
        tools:ignore="MissingConstraints,TextContrastCheck" />

    <Button
        android:id="@+id/asowner"
        android:layout_width="335dp"
        android:layout_height="50dp"

        android:layout_marginTop="68dp"
        android:background="@drawable/btn_bg"
        android:backgroundTint="@color/teal_700"
        android:text="owner"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:cornerRadius="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/asuser"
        tools:ignore="DuplicateSpeakableTextCheck,TextContrastCheck" />

    <Button
        android:id="@+id/asuser"
        android:layout_width="335dp"
        android:layout_height="50dp"

        android:layout_marginTop="36dp"
        android:background="@drawable/btn_bg"
        android:backgroundTint="@color/teal_700"
        android:text="USER"
        android:textAlignment="center"
        android:textAllCaps="true"


        android:textColor="@color/colorWhite"
        android:textStyle="bold"

        app:cornerRadius="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loginas"
        tools:ignore="DuplicateSpeakableTextCheck,TextContrastCheck " />

    <Button
        android:id="@+id/astiffinservice"
        android:layout_width="335dp"
        android:layout_height="50dp"

        android:layout_marginTop="60dp"
        android:background="@drawable/btn_bg"
        android:backgroundTint="@color/teal_700"


        android:text="Tiffin service"
        android:textAlignment="center"

        android:textAllCaps="true"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:cornerRadius="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/asowner" />

</androidx.constraintlayout.widget.ConstraintLayout>